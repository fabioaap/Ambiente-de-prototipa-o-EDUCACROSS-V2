{
  "name": "@prototipo/design-system",
  "version": "0.2.0-beta",
  "description": "Biblioteca de componentes React para o sistema de prototipação",
  "main": "dist/src/index.js",
  "module": "dist/src/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/src/index.mjs",
      "require": "./dist/src/index.js"
    },
    "./index.css": "./dist/src/index.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@prototipo/tokens": "workspace:*",
    "@radix-ui/react-dropdown-menu": "^2.1.16"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@prototipo/eslint-config": "workspace:*",
    "@storybook/react": "^8.4.7",
    "@storybook/types": "^8.6.14",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "esbuild-css-modules-plugin": "^3.1.5",
    "eslint": "^9.15.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  }
}
