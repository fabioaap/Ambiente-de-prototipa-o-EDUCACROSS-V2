{
  "name": "prototipacao-educa",
  "version": "0.2.0-beta",
  "private": true,
  "description": "Repositório de prototipação frontend orientado a jornadas com Puck, React e Storybook",
  "engines": {
    "node": "22.21.1",
    "pnpm": ">=9.14.4"
  },
  "scripts": {
    "dev:admin": "pnpm --filter admin dev",
    "dev:studio": "pnpm --filter studio dev",
    "dev:hub": "pnpm --filter storybook dev",
    "dev:storybook": "pnpm --filter storybook dev",
    "build:admin": "pnpm --filter admin build",
    "build:studio": "pnpm --filter studio build",
    "build:hub": "pnpm --filter storybook build",
    "build:storybook": "pnpm --filter storybook build",
    "build:tokens": "pnpm --filter @prototipo/tokens build",
    "build:design-system": "pnpm --filter @prototipo/design-system build",
    "build": "pnpm build:tokens && pnpm build:design-system && pnpm build:hub && pnpm build:studio && pnpm build:admin",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "test:dashboard-hydration": "pnpm --filter studio test:dashboard-hydration",
    "sync:figma": "node scripts/sync-figma-tokens.mjs",
    "check:shadcn": "node scripts/check-shadcn-usage.mjs",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "domains:index": "node scripts/generate-domains-index.mjs",
    "clean": "pnpm -r exec rm -rf node_modules dist build .next storybook-static",
    "gen:journeys": "node ./scripts/gen-journeys-index.js",
    "specfy": "npx @specfy/stack-analyser . --output=docs/specfy-stack.json"
  },
  "devDependencies": {
    "@prototipo/eslint-config": "workspace:*",
    "eslint": "^9.15.0",
    "prettier": "^3.3.3",
    "turbo": "^2.6.1",
    "typescript": "^5.6.3"
  },
  "packageManager": "pnpm@9.14.4",
  "dependencies": {
    "@specfy/stack-analyser": "1.27.3"
  }
}