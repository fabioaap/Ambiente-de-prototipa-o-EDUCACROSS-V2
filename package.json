{
  "name": "prototipacao-educa",
  "version": "0.1.0",
  "private": true,
  "description": "Repositório de prototipação frontend orientado a jornadas com Puck, React e Storybook",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev:studio": "pnpm --filter studio dev",
    "dev:storybook": "pnpm --filter storybook dev",
    "build:studio": "pnpm --filter studio build",
    "build:storybook": "pnpm --filter storybook build",
    "build:tokens": "pnpm --filter @prototipo/tokens build",
    "build:design-system": "pnpm --filter @prototipo/design-system build",
    "build": "pnpm build:tokens && pnpm build:design-system && pnpm build:studio && pnpm build:storybook",
    "lint": "pnpm -r lint",
    "setup:gh:labels": "bash ./scripts/gh/create-labels.sh",
    "setup:gh:issues": "bash ./scripts/gh/create-issues.sh",
    "setup:gh:issues:all": "bash ./scripts/gh/create-issues-all.sh",
    "setup:gh:project": "bash ./scripts/gh/create-project.sh",
    "setup:gh:add-issues-to-project": "bash ./scripts/gh/add-issues-to-project.sh",
    "setup:gh:full": "bash ./scripts/gh/setup-github-full.sh",
    "test": "pnpm -r test",
    "clean": "pnpm -r exec rm -rf node_modules dist build .next storybook-static"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "prettier": "^3.3.3",
    "eslint": "^9.15.0",
    "@eslint/js": "^9.15.0",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0"
  },
  "packageManager": "pnpm@9.14.4"
}
